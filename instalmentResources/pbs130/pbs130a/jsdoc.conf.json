{
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "readme": "./README.md",
        "recurse": true
    }, 
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": ["src/"],
        "includePattern": ".+\\.(mjs|jsdoc)$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
