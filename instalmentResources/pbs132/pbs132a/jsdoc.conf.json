{
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "tutorials": "./pages/"
    },
    "source": {
        "include": ["./src/"],
        "includePattern": ".+\\.(mjs|jsdoc)$"
    }, 
    "plugins": [
        "plugins/markdown"
    ],
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    }
}