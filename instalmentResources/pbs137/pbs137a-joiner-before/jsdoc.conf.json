{
    "docdash": {
        "collapse": true,
        "search": true,
        "sectionOrder": ["Modules", "Classes", "Externals"],
        "static": true,
        "typedefs": true
    },
    "opts": {
        "encoding": "utf8",
        "pedantic": true,
        "readme": "./README.md"
    },
    "source": {
        "include": ["./src/"],
        "includePattern": ".+\\.js(doc)?$"
    }, 
    "plugins": [
        "plugins/markdown"
    ],
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    }
}